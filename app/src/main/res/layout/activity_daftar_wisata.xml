<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9EC0E8"
    tools:context=".DaftarWisataActivity">

    <ImageView
        android:id="@+id/btn_arrow_back"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/arrow_back_foreground"
        android:backgroundTint="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_editor_absoluteX="13dp"
        app:layout_editor_absoluteY="20dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="169dp"
        android:layout_height="54dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Jelajahi Destinasi Wisata Pilihan"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_arrow_back"
        app:layout_editor_absoluteX="24dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="72dp"
        android:background="@drawable/foto"
        android:contentDescription="@string/foto"
        app:layout_constraintBottom_toTopOf="@+id/searchView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/v_provinsi"
        android:layout_width="183dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/kotak_provinsi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/tv_kotakprovinsi"
        android:layout_width="172dp"
        android:layout_height="18dp"
        android:layout_marginStart="32dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Nanggroe Aceh Darussalam"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/v_provinsi"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchView"
        android:layout_width="330dp"
        android:layout_height="30dp"
        android:layout_margin="10dp"
        android:background="@drawable/search_bar"
        app:iconifiedByDefault="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_provinsi"
        app:queryHint="Cari Nama Provinsi" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="180dp"
        android:layout_height="95dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/museum_tsunami_aceh"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="214dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="175dp"
        android:layout_height="39dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Museum Tsunami Aceh"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="297dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="175dp"
        android:layout_height="30dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Kec. Baiturrahman, Kota Banda Aceh, Aceh"
        android:textColor="@color/black"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="300dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="326dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="180dp"
        android:layout_height="95dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/pantan_terong"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="345dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="133dp"
        android:layout_height="39dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Pantan Terong"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="428dp" />

    <TextView
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Kec. Bebesan, Kab. Aceh Tengah, Aceh"
        android:textColor="@color/black"
        android:textSize="9sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_editor_absoluteX="20dp"
        app:layout_editor_absoluteY="442dp" />

    <ImageView
        android:id="@+id/imageButton"
        android:layout_width="180dp"
        android:layout_height="95dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="28dp"
        android:src="@drawable/pantai_lampuuk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchView"
        app:layout_editor_absoluteX="220dp"
        app:layout_editor_absoluteY="214dp" />

    <TextView
        android:layout_width="146dp"
        android:layout_height="39dp"
        android:layout_marginEnd="60dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Pantai Lampuuk"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_editor_absoluteX="220dp"
        app:layout_editor_absoluteY="297dp"
        tools:layout_editor_absoluteY="310dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="160dp"
        android:layout_height="30dp"
        android:layout_marginEnd="45dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Kec. Lhonknga, Kab. Aceh Besar, Aceh."
        android:textColor="@color/black"
        android:textSize="9sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_editor_absoluteX="225dp"
        app:layout_editor_absoluteY="310dp"
        tools:layout_editor_absoluteY="326dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>